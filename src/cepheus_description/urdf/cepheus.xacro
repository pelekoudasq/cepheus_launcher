	<joint name="p1_joint" type="prismatic">
		<parent link="world"/>
		<child link="p1"/>
		<axis xyz="1 0 0"/>
		<limit lower="-100" upper="100" effort="1000" velocity="10000"/>
		<dynamics damping="0" friction="0"/>
	</joint>
	<link name="p1">
		<inertial>
			<origin
				xyz="0.0 0.0 0.0"
				rpy="0.0 0.0 0.0" />
			<mass value="0.01" />
			<inertia
				ixx="1e-2"
				ixy="0.0"
				ixz="0.0"
				iyy="1e-2"
				iyz="0.0"
				izz="1e-2" />
		</inertial>
	</link>
	<gazebo reference="p1">
		<mu1>0</mu1>
		<mu2>0</mu2>
	</gazebo>
	<transmission name="lin1">
		<type>transmission_interface/SimpleTransmission</type>
		<joint name="p1_joint">
			<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		</joint>
		<actuator name="lin_motor1">
			<hardwareInterface>EffortJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
	<joint name="p2_joint" type="prismatic">
		<parent link="p1"/>
		<child link="p2"/>
		<axis xyz="0 1 0"/>
		<limit
			lower="-100"
			upper="100"
			effort="1000"
			velocity="10000"/>
		<dynamics
			damping="0"
			friction="0"/>
	</joint>
	<link name="p2">
		<inertial>
			<origin
				xyz="0.0 0.0 0.0"
				rpy="0.0 0.0 0.0" />
			<mass value="0.01" />
			<inertia
