			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
	<joint name="r_joint" type="continuous">
		<axis xyz="0 0 1"/>
		<parent link="p2"/>
		<origin rpy="0 0 0" xyz="0 0 0"/>
		<child link="cepheus"/>
	</joint>
